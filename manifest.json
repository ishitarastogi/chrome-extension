{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "browser_action": {
    "default_popup": "rocket-p5js/index.html",
    "default_icon": "logo.png",
    "default_title": "launch rocket"
  },
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"]
}
